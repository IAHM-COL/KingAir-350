				<max>1.5</max>
			</volume>
			<pitch>
				<property>engines/engine[1]/rpm</property>
				<factor>0.000055</factor>
				<min>0.0</min>
				<max>2.6</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m> 2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</enginehi>

		<enginelo>
			<name>engine-loR</name>
			<mode>looped</mode>
			<path>Aircraft/KingAir-350/Sounds/iar-slow.wav</path>
