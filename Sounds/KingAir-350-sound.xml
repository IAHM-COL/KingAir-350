		<!-- WIND -->
		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<property>velocities/airspeed-kt</property>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>0.05</offset>
				<min>0.01</min>
				<max>0.7</max>
			</volume>
			<volume>
				<property>surface-positions/flap-pos-norm</property>
				<factor>0.03</factor>
				<offset>0.05</offset>
				<min>0.01</min>
				<max>0.7</max>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>0.5</offset>
				<max>1.0</max>
			</pitch>
			<reference-dist>2</reference-dist>
			<max-dist>5</max-dist>
		</wind>

		<!-- AIRFRAME TORSION -->
		<high-g>
			<name>high-g</name>
			<mode>in-transit</mode>
			<path>Aircraft/KingAir-350/Sounds/airframe-torsion.wav</path>
			<condition>
				<or>
					<greater-than>
						<property>accelerations/pilot-g</property>
						<value>2.2</value>
					</greater-than>
					<less-than>
						<property>accelerations/pilot-g</property>
						<value>-0.9</value>
					</less-than>
				</or>
			</condition>
			<volume>
				<property>accelerations/pilot-g</property>
				<factor>0.25</factor>
			</volume>
			<pitch>
				<property>accelerations/pilot-g</property>
				<factor>0.25</factor>
				<max>1.5</max>
			</pitch>
			<position>
				<x-m> 2.573</x-m>
				<y-m> 0.000</y-m>
				<z-m>-0.200</z-m>
			</position>
			<reference-dist>2</reference-dist>
			<max-dist>5</max-dist>
