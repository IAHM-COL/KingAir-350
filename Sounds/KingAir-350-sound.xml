		<!-- ENGINE COUGH -->
		<enginecough>
			<name>coughR</name>
			<path>Aircraft/KingAir-350/Sounds/coughing.wav</path>
			<type>raise</type>
			<condition>
				<and>
					<greater-than>
						<property>engines/engine[1]/rpm</property>
						<value>500</value>
					</greater-than>
					<not>
						<property>engines/engine[1]/running</property>
					</not>
					<not>
						<property>engines/engine[1]/cranking</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>engines/engine[1]/rpm</property>
				<factor>0.01</factor>
				<offset>-0.20</offset>
				<min>0.15</min>
				<max>0.8</max>
			</volume>
			<pitch>
				<property>engines/engine[1]/rpm</property>
				<factor>0.00000025</factor>
				<min>0.0</min>
				<max>1.5</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m> 2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</enginecough>

		<!-- GEAR RUMBLE -->
		<rumbleL>
			<name>rumble-left</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<and>
					<property>gear/gear[0]/wow</property>
					<greater-than>
						<property>gear/gear[0]/rollspeed-ms</property>
						<value>1</value>
					</greater-than>
				</and>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<type>log</type>
				<factor>0.08</factor>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<type>log</type>
				<factor>0.75</factor>
			</pitch>
			<position>
				<x-m> 2.573</x-m>
				<y-m>-1.637</y-m>
				<z-m>-1.595</z-m>
			</position>
			<reference-dist>3</reference-dist>
			<max-dist>10</max-dist>
		</rumbleL>

		<rumbleR>
			<name>rumble-right</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<property>gear/gear[1]/wow</property>
				<greater-than>
					<property>gear/gear[1]/rollspeed-ms</property>
					<value>1</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<type>log</type>
				<factor>0.08</factor>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<type>log</type>
				<factor>0.75</factor>
			</pitch>
			<position>
				<x-m> 2.573</x-m>
				<y-m> 1.637</y-m>
				<z-m>-1.595</z-m>
			</position>
			<reference-dist>3</reference-dist>
			<max-dist>10</max-dist>
		</rumbleR>

		<!-- GEAR TRANSIT -->
		<gearL>
			<name>gearL</name>
			<mode>in-transit</mode>
			<path>Sounds/flaps.wav</path>
			<property>gear/gear[0]/position-norm</property>
			<volume>0.2</volume>
			<pitch>
				<property>gear/gear[0]/position-norm</property>
				<factor>0.9</factor>
				<offset>-0.1</offset>
				<min>0.7</min>
				<max>0.85</max>
			</pitch>
			<position>
				<x-m> 2.573</x-m>
				<y-m>-1.637</y-m>
				<z-m>-0.595</z-m>
			</position>
			<reference-dist>3</reference-dist>
			<max-dist>10</max-dist>
		</gearL>

		<gearlL>
			<name>gear-lockL</name>
			<path>Sounds/gear-lck.wav</path>
			<condition>
				<greater-than>
					<property>gear/gear[0]/position-norm</property>
					<value>0.95</value>
				</greater-than>
			</condition>
			<volume>0.5</volume>
			<position>
				<x-m> 2.573</x-m>
				<y-m>-1.637</y-m>
				<z-m>-0.595</z-m>
			</position>
			<reference-dist>2</reference-dist>
			<max-dist>5</max-dist>
		</gearlL>

		<gearR>
			<name>gearR</name>
			<mode>in-transit</mode>
			<path>Sounds/flaps.wav</path>
			<property>gear/gear[1]/position-norm</property>
			<volume>0.2</volume>
			<pitch>
				<property>gear/gear[1]/position-norm</property>
				<factor>0.9</factor>
				<offset>-0.1</offset>
				<min>0.7</min>
				<max>0.85</max>
			</pitch>
			<position>
