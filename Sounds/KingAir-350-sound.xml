			<volume>0.8</volume>
			<pitch>
				<property>engines/engine[1]/cranking</property>
				<factor>0.8</factor>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m> 2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</crank>

		<!-- ENGINE RUNNING -->
		<enginehi>
			<name>engine-hiR</name>
			<mode>looped</mode>
			<path>Aircraft/KingAir-350/Sounds/iar-running-m.wav</path>
			<condition>
				<property>engines/engine[1]/running</property>
				<less-than>
					<property>engines/engine[1]/rpm</property>
