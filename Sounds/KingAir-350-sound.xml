				<y-m> 2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</engine-idle>

		<!-- ENGINE STOP -->
		<enginestop>
			<name>engineoffR</name>
			<path>Aircraft/KingAir-350/Sounds/shutdown.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>engines/engine[1]/rpm</property>
