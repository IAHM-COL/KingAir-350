<?xml version="1.0"?>

<PropertyList>
	<fx>
		<!-- LEFT ENGINE -->
		<!-- ENGINE START -->
		<crank>
			<name>crankL</name>
			<path>Aircraft/KingAir-350/Sounds/starter.wav</path>
			<condition>
				<property>engines/engine[0]/cranking</property>
				<less-than>
					<property>engines/engine[0]/rpm</property>
					<value>350</value>
				</less-than>
			</condition>
			<volume>0.8</volume>
			<pitch>
				<property>engines/engine[0]/cranking</property>
				<factor>0.8</factor>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m>-2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</crank>

	<!-- ENGINE RUNNING -->
		<enginehi>
			<name>engine-hiL</name>
			<mode>looped</mode>
			<path>Aircraft/KingAir-350/Sounds/iar-running-m.wav</path>
			<condition>
				<property>engines/engine[0]/running</property>
				<less-than>
					<property>engines/engine[0]/rpm</property>
					<value>5000</value>
				</less-than>
			</condition>
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.003</factor>
				<offset>-0.4</offset>
				<min>0.0</min>
				<max>1.5</max>
			</volume>
			<pitch>
				<property>engines/engine[0]/rpm</property>
				<factor>0.000055</factor>
				<min>0.0</min>
				<max>2.6</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m>-2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</enginehi>

		<enginelo>
			<name>engine-loL</name>
			<mode>looped</mode>
			<path>Aircraft/KingAir-350/Sounds/iar-slow.wav</path>
			<condition>
				<property>engines/engine[0]/running</property>
				<greater-than>
					<property>engines/engine[0]/rpm</property>
					<value>110</value>
				</greater-than>
			</condition>
			<volume>
				<property>engines/engine[0]/mp-inhg</property>
				<factor>0.04</factor>
				<offset>-0.35</offset>
				<min>0.0</min>
				<max>1.1</max>
			</volume>
			<pitch>
				<property>engines/engine[0]/rpm</property>
				<factor>0.000015</factor>
				<min>0.0</min>
				<max>5.1</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m>-2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</enginelo>

		<engine-idle>
			<name>engine-idleL</name>
			<mode>looped</mode>
			<path>Aircraft/KingAir-350/Sounds/engine.wav</path>
			<condition>
				<property>engines/engine[0]/running</property>
				<greater-than>
					<property>engines/engine[0]/rpm</property>
					<value>100</value>
				</greater-than>
			</condition>
			<volume>
				<property>engines/engine[0]/mp-inhg</property>
				<offset>0.9</offset>
				<factor>-0.03</factor>
				<min>0.3</min>
				<max>0.9</max>
			</volume>
			<pitch>
				<property>engines/engine[0]/rpm</property>
				<factor>0.001</factor>
				<offset>0.7</offset>
				<min>0.7</min>
				<max>5.1</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m>-2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</engine-idle>

		<!-- ENGINE STOP -->
		<enginestop>
			<name>engineoffL</name>
			<path>Aircraft/KingAir-350/Sounds/shutdown.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>engines/engine[0]/rpm</property>
						<value>50</value>
					</greater-than>
					<not>
						<property>engines/engine[0]/running</property>
					</not>
					<not>
						<property>engines/engine[0]/cranking</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>engines/engine[0]/rpm</property>
				<factor>0.01</factor>
				<offset>-0.20</offset>
				<min>0.15</min>
				<max>0.8</max>
			</volume>
			<pitch>
				<property>engines/engine[0]/rpm</property>
				<factor>0.00000025</factor>
				<min>0.0</min>
				<max>1.5</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m>-2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</enginestop>

		<!-- ENGINE COUGH -->
		<enginecough>
			<name>coughL</name>
			<path>Aircraft/KingAir-350/Sounds/coughing.wav</path>
			<type>raise</type>
			<condition>
				<and>
					<greater-than>
						<property>engines/engine[0]/rpm</property>
						<value>500</value>
					</greater-than>
					<not>
						<property>engines/engine[0]/running</property>
					</not>
					<not>
						<property>engines/engine[0]/cranking</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>engines/engine[0]/rpm</property>
				<factor>0.01</factor>
				<offset>-0.20</offset>
				<min>0.15</min>
				<max>0.8</max>
			</volume>
			<pitch>
				<property>engines/engine[0]/rpm</property>
				<factor>0.00000025</factor>
				<min>0.0</min>
				<max>1.5</max>
			</pitch>
			<position>
				<x-m>-0.815</x-m>
				<y-m>-2.1</y-m>
				<z-m>-0.53</z-m>
			</position>
		</enginecough>

		<!-- RIGHT ENGINE -->
		<!-- ENGINE START -->
		<crank>
			<name>crankR</name>
			<path>Aircraft/KingAir-350/Sounds/starter.wav</path>
			<condition>
				<property>engines/engine[1]/cranking</property>
