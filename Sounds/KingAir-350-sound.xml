		</enginelo>

		<engine-idle>
			<name>engine-idleR</name>
			<mode>looped</mode>
			<path>Aircraft/KingAir-350/Sounds/engine.wav</path>
			<condition>
				<property>engines/engine[1]/running</property>
				<greater-than>
					<property>engines/engine[1]/rpm</property>
					<value>100</value>
				</greater-than>
			</condition>
			<volume>
				<property>engines/engine[1]/mp-inhg</property>
